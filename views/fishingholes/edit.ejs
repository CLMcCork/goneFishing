<% layout('layouts/boilerplate') %> 
<h1>Edit Fishing Hole</h1>
<form action="/fishingholes/<%= fishinghole._id %>?_method=PUT" method ="POST">
    <div>
        <label for="title">Title</label>
        <input type="text" id="title" name="fishinghole[title]" value="<%= fishinghole.title %>">
    </div>
    <div>
        <label for="location">Location</label>
        <input type="text" id="location" name="fishinghole[location]" value="<%= fishinghole.location %>">
    </div>
    <button>Update Fishing Hole</button>
</form>
    <a href="/fishingholes/<%= fishinghole._id %>">Back to Fishing Hole</a>
